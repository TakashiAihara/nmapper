{
  "extends": [
    "../../.eslintrc.json",
    "plugin:react-hooks/recommended",
    "plugin:react-refresh/recommended"
  ],
  "plugins": ["react-refresh"],
  "env": {
    "browser": true,
    "es2020": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react-refresh/only-export-components": [
      "warn",
      { "allowConstantExport": true }
    ],
    "@typescript-eslint/explicit-function-return-type": "off"
  },
  "settings": {
    "react": {
      "version": "18.3"
    }
  }
}